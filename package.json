{
  "name": "there-web",
  "version": "0.0.1",
  "dependencies": {
    "babel-plugin-styled-components": "1.5.1",
    "moment-timezone": "0.5.20",
    "ms": "2.1.1",
    "next": "6.0.3",
    "nprogress": "0.2.0",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-onclickout": "2.0.8",
    "react-pose": "2.1.2",
    "styled-components": "3.3.2"
  },
  "devDependencies": {
    "@types/next": "6.0.1",
    "@types/prop-types": "15.5.3",
    "babel-eslint": "8.2.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.9.1",
    "husky": "0.14.3",
    "lint-staged": "7.2.0",
    "prettier": "1.13.5",
    "prop-types": "15.6.1",
    "react-testing-library": "4.0.1",
    "xo": "0.21.1"
  },
  "xo": {
    "parser": "babel-eslint",
    "extends": [
      "prettier",
      "prettier/react",
      "plugin:react/recommended"
    ],
    "env": [
      "browser",
      "node"
    ],
    "rules": {
      "no-await-in-loop": 0,
      "react/react-in-jsx-scope": 0,
      "import/no-unresolved": 0,
      "import/no-extraneous-dependencies": 0,
      "complexity": 0,
      "lines-between-class-members": 0,
      "unicorn/filename-case": 0,
      "react/prop-types": 0
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn test-lint",
      "prettier",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "scripts": {
    "dev": "next -p 6222",
    "build": "next build && next export",
    "start": "next start -p 6222",
    "precommit": "lint-staged",
    "test-lint": "xo"
  }
}
